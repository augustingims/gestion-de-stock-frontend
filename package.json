    "gs-api:swagger-gen": "./node_modules/.bin/ng-swagger-gen -i ./tools/swagger/src/swagger.json -o src/gs-api/src",
    "dev:ssr": "ng run gestion-de-stock:serve-ssr",
    "serve:ssr": "node dist/functions/server/main.js",
    "build:ssr": "ng build --prod && ng run gestion-de-stock:server:production && npm run compile:server",
    "prerender": "ng run gestion-de-stock:prerender",
    "pwa": "ngsw-config dist/functions/browser ngsw-config.json",
    "compile:server": "webpack --config server.webpack.config.js --progress ",
    "build:prerender": "npm run build:ssr && node dist/functions/prerender",
