    "gs-api:create-dist": "mkdir -p tools/swagger/dist && mkdir -p tools/swagger/src",
    "gs-api:download": "cp /Users/mac/Desktop/projets/Gestion\\ de\\ stock/backend/gestiondestock/target/gestiondestock-0.0.1-SNAPSHOT.jar ./tools/swagger/dist && cp /Users/mac/Desktop/projets/Gestion\\ de\\ stock/backend/gestiondestock/target/swagger.json ./tools/swagger/src",
