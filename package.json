    "gs-api:create-dist": "mkdir -p tools/swagger/dist && mkdir -p tools/swagger/src",
    "gs-api:download": "cp /Users/mac/Desktop/projets/Gestion\\ de\\ stock/backend/gestiondestock/target/gestiondestock-0.0.1-SNAPSHOT.jar ./tools/swagger/dist && cp /Users/mac/Desktop/projets/Gestion\\ de\\ stock/backend/gestiondestock/target/swagger.json ./tools/swagger/src",
    "gs-api:extract": "node ./tools/swagger/src/swagger.json 0.0.1-SNAPSHOT",
    "gs-api:swagger-gen": "./node_modules/.bin/ng-swagger-gen -i ./tools/swagger/src/swagger.json -o src/gs-api/src",
    "dev:ssr": "ng run gestion-de-stock:serve-ssr",
    "serve:ssr": "node dist/gestion-de-stock/server/main.js",
    "build:ssr": "ng build --prod && ng run gestion-de-stock:server:production",
    "prerender": "ng run gestion-de-stock:prerender"
  },
